{
  "manifest_version": 2,

  "name": "Polipics",
  "description": "This extension changes images of the current page from a form",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/Polipics.png",
    "default_popup": "formulaire.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
